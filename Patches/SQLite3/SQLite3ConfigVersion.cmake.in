set( PACKAGE_VERSION @SQLite3_VERSION@ )

# If no version requested then assume compatibility
if( NOT PACKAGE_FIND_VERSION )
  set( PACKAGE_VERSION_COMPATIBLE 1 )
elseif( PACKAGE_FIND_VERSION VERSION_EQUAL @SQLite3_VERSION@ )
  set( PACKAGE_VERSION_EXACT TRUE )
  set( PACKAGE_VERSION_COMPATIBLE 1 )
elseif( PACKAGE_FIND_VERSION VERSION_LESS @SQLite3_VERSION@ )
  set( PACKAGE_VERSION_COMPATIBLE 1 )
else()
  set( PACKAGE_VERSION_UNSUITABLE 1 )
endif()
